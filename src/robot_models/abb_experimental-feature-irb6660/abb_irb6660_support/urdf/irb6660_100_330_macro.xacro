<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:include filename="$(find abb_resources)/urdf/common_materials.xacro"/>

  <xacro:macro name="abb_irb6660_100_330" params="prefix">
    <!-- link list -->
    <link name="${prefix}base_link">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://abb_irb6660_support/meshes/irb6660/visual/base_link.stl"/>
          </geometry>
        <xacro:material_abb_orange />
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://abb_irb6660_support/meshes/irb6660/collision/base_link.stl"/>
        </geometry>
        <xacro:material_abb_yellow />
      </collision>
    </link>
    <link name="${prefix}link_1">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://abb_irb6660_support/meshes/irb6660/visual/link_1.stl"/>
        </geometry>
        <xacro:material_abb_orange />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://abb_irb6660_support/meshes/irb6660/collision/link_1.stl"/>
        </geometry>
        <xacro:material_abb_yellow />
      </collision>
    </link>
    <link name="${prefix}link_2">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://abb_irb6660_support/meshes/irb6660_100_330/visual/link_2.stl"/>
        </geometry>
        <xacro:material_abb_orange />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://abb_irb6660_support/meshes/irb6660_100_330/collision/link_2.stl"/>
        </geometry>
        <xacro:material_abb_yellow />
      </collision>
    </link>
    <link name="${prefix}link_3">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://abb_irb6660_support/meshes/irb6660/visual/link_3.stl"/>
        </geometry>
        <xacro:material_abb_orange />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://abb_irb6660_support/meshes/irb6660/collision/link_3.stl"/>
        </geometry>
        <xacro:material_abb_yellow />
      </collision>
    </link>
    <link name="${prefix}link_4">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://abb_irb6660_support/meshes/irb6660_100_330/visual/link_4.stl"/>
        </geometry>
        <xacro:material_abb_orange />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://abb_irb6660_support/meshes/irb6660_100_330/collision/link_4.stl"/>
        </geometry>
        <xacro:material_abb_yellow />
      </collision>
    </link>
    <link name="${prefix}link_5">
      <visual>
        <origin xyz="-0.15 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://abb_irb6660_support/meshes/irb6660_100_330/visual/link_5.stl"/>
        </geometry>
        <xacro:material_abb_orange />
      </visual>
      <collision>
        <origin xyz="-0.15 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://abb_irb6660_support/meshes/irb6660_100_330/collision/link_5.stl"/>
        </geometry>
        <xacro:material_abb_yellow />
      </collision>
    </link>
    <link name="${prefix}link_6">
      <visual>
        <origin xyz="-0.15 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://abb_irb6660_support/meshes/irb6660/visual/link_6.stl"/>
        </geometry>
        <xacro:material_abb_black />
      </visual>
      <collision>
        <origin xyz="-0.15 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://abb_irb6660_support/meshes/irb6660/collision/link_6.stl"/>
        </geometry>
        <xacro:material_abb_black />
      </collision>
    </link>
    <link name="${prefix}tool0"/>
    <!-- end of link list -->

    <!-- joint list -->
    <joint name="${prefix}joint_1" type="revolute">
      <origin xyz="0 0 0.8145" rpy="0 0 0"/>
      <parent link="${prefix}base_link"/>
      <child link="${prefix}link_1"/>
      <axis xyz="0 0 1"/>
      <limit lower="-${radians(180)}" upper="${radians(180)}" effort="0" velocity="${radians(110)}"/>
      </joint>
    <joint name="${prefix}joint_2" type="revolute">
      <origin xyz="0.300 0 0" rpy="0 0 0"/>
      <parent link="${prefix}link_1"/>
      <child link="${prefix}link_2"/>
      <axis xyz="0 1 0"/>
      <limit lower="-${radians(150)}" upper="${radians(150)}" effort="0" velocity="${radians(130)}"/>
    </joint>
    <joint name="${prefix}joint_3" type="revolute">
      <origin xyz="0 0 1.280" rpy="0 0 0"/>
      <parent link="${prefix}link_2"/>
      <child link="${prefix}link_3"/>
      <axis xyz="0 1 0"/>
      <limit lower="-${radians(20)}" upper="${radians(120)}" effort="0" velocity="${radians(130)}"/>
    </joint>
    <joint name="${prefix}joint_4" type="revolute">
      <origin xyz="0 0 0.280" rpy="0 0 0"/>
      <parent link="${prefix}link_3"/>
      <child link="${prefix}link_4"/>
      <axis xyz="1 0 0"/>
      <limit lower="-${radians(50)}" upper="${radians(50)}" effort="0" velocity="${radians(150)}"/>
    </joint>
    <joint name="${prefix}joint_5" type="revolute">
      <origin xyz="1.7555 0 0" rpy="0 0 0"/>
      <parent link="${prefix}link_4"/>
      <child link="${prefix}link_5"/>
      <axis xyz="0 1 0"/>
      <limit lower="-${radians(90)}" upper="${radians(90)}" effort="0" velocity="${radians(120)}"/>
    </joint>
    <joint name="${prefix}joint_6" type="revolute">
      <origin xyz="0.200 0 0" rpy="0 0 0"/>
      <parent link="${prefix}link_5"/>
      <child link="${prefix}link_6"/>
      <axis xyz="1 0 0"/>
      <limit lower="-${radians(170)}" upper="${radians(170)}" effort="0" velocity="${radians(240)}"/>
    </joint>
    <joint name="${prefix}joint_6-tool0" type="fixed">
      <origin xyz="0 0 0" rpy="0 ${radians(90)} 0"/>
      <parent link="${prefix}link_6"/>
      <child link="${prefix}tool0"/>
    </joint>
    <!-- end of joint list -->

    <!-- ROS base_link to ABB World Coordinates transform -->
    <link name="${prefix}base" />
    <joint name="${prefix}base_link-base" type="fixed">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <parent link="${prefix}base_link"/>
      <child link="${prefix}base"/>
    </joint>
  </xacro:macro>
</robot>
